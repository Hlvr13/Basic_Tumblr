<h1 class="">Make somethin</h1>

<%= form_for :post, url: posts_path do |f| %>
    <p>
        <%= f.label :title %><br>
        <%= f.text_field :title %>
    </p>

    <p class="">
        <%= f.label :body %> <br>
        <%= f.text_area :body %>
    </p>

    <p class="">
        <%= f.submit %>
    </p>

<% end %>